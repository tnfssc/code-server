{
  "workbench.startupEditor": "none",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "sqltools.connections": [],
  "github.copilot.enable": {
    "*": true
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "code-eol.decorateBeforeEol": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "workbench.iconTheme": "material-icon-theme",
  "files.exclude": {
    "**/.project": true,
    "**/.settings": true,
    "**/.classpath": true,
    "**/.factorypath": true
  },
  "editor.accessibilitySupport": "off",
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "git.confirmSync": false,
  "eslint.alwaysShowStatus": true,
  "go.toolsManagement.autoUpdate": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.watcherExclude": {
    "**/.vercel/**": true,
    "**/.next/**": true
  },
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "terminal.integrated.tabs.enabled": true,
  "eslint.packageManager": "yarn",
  "prettier.bracketSameLine": true,
  "redhat.telemetry.enabled": false,
  "workbench.activityBar.visible": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.printWidth": 120,
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'MesloLGS NF'",
  "workbench.enableExperiments": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "update.mode": "start",
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.fontSize": 14,
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "editor.renderWhitespace": "none",
  "[vanillajavascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "[cpp]": {
    "editor.defaultFormatter": "jeff-hykin.better-cpp-syntax"
  },
  "python.formatting.provider": "black",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.guides.bracketPairs": "active",
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "git.autofetch": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.fontLigatures": true,
  "workbench.colorTheme": "SkillAvid Pure Black",
  "workbench.editor.untitled.hint": "hidden",
  "python.defaultInterpreterPath": "/usr/bin/python",
  "typescript.disableAutomaticTypeAcquisition": true,
  "terminal.integrated.inheritEnv": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "notebook.lineNumbers": "on",
  "editor.suggestSelection": "first",
  "git.enableSmartCommit": true,
  "explorer.confirmDelete": false,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.eol": "\n",
  "editor.fontFamily": "'FiraCode Nerd Font', 'MesloLGS NF', 'Courier New', monospace",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "update.showReleaseNotes": false,
  "editor.fontSize": 15,
  "eslint.format.enable": true,
  "prettier.arrowParens": "avoid",
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "files.associations": {
    "*.js": "javascript"
  },
  "workbench.settings.enableNaturalLanguageSearch": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "[php]": {
    "editor.defaultFormatter": "felixfbecker.php-intellisense"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "cmake.configureOnOpen": false,
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "cSpell.userWords": [
    "astro",
    "azuretools",
    "chakra",
    "chartjs",
    "concat",
    "conv",
    "corepack",
    "Creds",
    "crunker",
    "cupertino",
    "Deepgram",
    "gmeet",
    "headlessui",
    "jciq",
    "kafkajs",
    "kompose",
    "kubectl",
    "Kubernetes",
    "kustomize",
    "libreddit",
    "middlewares",
    "minio",
    "nextui",
    "nuxt",
    "nuxtjs",
    "Plyr",
    "rustdesk",
    "rxchain",
    "searxng",
    "sharath",
    "skaffold",
    "stylelint",
    "surrealdb",
    "tailwindcss",
    "tnfssc",
    "undici",
    "webdev",
    "wordcloud",
    "wouter",
    "zustand"
  ],
  "security.workspace.trust.untrustedFiles": "open",
  "css.lint.unknownAtRules": "ignore",
  "workbench.productIconTheme": "material-product-icons",
  "terminal.integrated.shellIntegration.enabled": true,
  "editor.tokenColorCustomizations": {
    "comments": "#a2ff0097"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.Go"
  },
  "go.inlayHints.constantValues": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.assignVariableTypes": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.parameterNames": true,
  "go.inlayHints.rangeVariableTypes": true,
  "go.survey.prompt": false,
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "editor.fontWeight": "normal",
  "[dockercompose]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  }
}
